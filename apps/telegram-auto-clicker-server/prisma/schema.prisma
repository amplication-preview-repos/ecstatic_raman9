datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Game {
  id            String         @id @default(cuid())
  createdAt     DateTime       @default(now())
  updatedAt     DateTime       @updatedAt
  name          String?
  url           String?
  commands      Json?
  commandQueues CommandQueue[]
}

model CommandQueue {
  id         String                  @id @default(cuid())
  createdAt  DateTime                @default(now())
  updatedAt  DateTime                @updatedAt
  command    String?
  executedAt DateTime?
  status     EnumCommandQueueStatus?
  user       User?                   @relation(fields: [userId], references: [id])
  userId     String?
  game       Game?                   @relation(fields: [gameId], references: [id])
  gameId     String?
}

model User {
  id               String         @id @default(cuid())
  createdAt        DateTime       @default(now())
  updatedAt        DateTime       @updatedAt
  firstName        String?
  lastName         String?
  username         String         @unique
  email            String?        @unique
  password         String
  roles            Json
  session          Json?
  proxy            String?
  telegramId       Int?
  userName         String?
  sessionData      Json?
  proxyAddress     String?
  commandQueues    CommandQueue[]
  telegramUserId   Int?
  telegramUserName String?
}

enum EnumCommandQueueStatus {
  Option1
}
